{
  "name": "@designgreat/design-token-support",
  "version": "0.2.4",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "pnpm run clean && tsc -p tsconfig.build.json",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "lint": "pnpm exec eslint . --max-warnings 0",
    "test": "pnpm exec vitest run",
    "clean": "rm -rf dist tsconfig*.tsbuildinfo",
    "prepare": "if [ \"$CI\" != \"true\" ]; then pnpm run build || (echo \"ERROR: This package must be installed without --prod\" && exit 1); fi",
    "prepublishOnly": "if [ \"$CI\" != \"true\" ]; then pnpm run build; fi"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@designgreat/lib-web-ui-design-token": "workspace:*"
  },
  "devDependencies": {
    "vitest": "^1.5.0"
  }
}
